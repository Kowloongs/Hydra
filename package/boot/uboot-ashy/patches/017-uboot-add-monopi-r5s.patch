+
+config SYS_VENDOR
+        default "ombres"
+
+config SYS_CONFIG_NAME
+        default "monopi-r5s-rk3568"
+
+config BOARD_SPECIFIC_OPTIONS # dummy
+        def_bool y
+
+endif
+
+
