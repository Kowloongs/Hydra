diff --git a/arch/arm/dts/Makefile b/arch/arm/dts/Makefile
index f8cbd3a8..4bfba191 100644
--- a/arch/arm/dts/Makefile
+++ b/arch/arm/dts/Makefile
@@ -167,7 +167,8 @@ dtb-$(CONFIG_ROCKCHIP_RK3568) += \
 	rk3568-bpi-r2-pro.dtb \
 	rk3568-evb.dtb \
 	rk3566-quartz64-a.dtb \
-	rk3568-rock-3a.dtb
+	rk3568-rock-3a.dtb \
+	rk3568-monopi-r5s.dtb
 
 dtb-$(CONFIG_ROCKCHIP_RV1108) += \
 	rv1108-elgin-r1.dtb \
diff --git a/arch/arm/dts/rk3568-monopi-r5s-u-boot.dtsi b/arch/arm/dts/rk3568-monopi-r5s-u-boot.dtsi
new file mode 100644
index 00000000..b4772fcb
--- /dev/null
+++ b/arch/arm/dts/rk3568-monopi-r5s-u-boot.dtsi
@@ -0,0 +1,26 @@
+//
+// Copyright (C) 2015 - 2029. Hydra Project.
+//
+
+#include "rk3568-u-boot.dtsi"
+
+/ {
+	chosen {
+		stdout-path = &uart2;
+		u-boot,spl-boot-order = "same-as-spl", &sdhci, &sdmmc0;
+	};
+};
+
+&sdmmc0 {
+	bus-width = <4>;
+	u-boot,dm-spl;
+	u-boot,spl-fifo-mode;
+};
+
+&uart2 {
+	clock-frequency = <24000000>;
+	u-boot,dm-spl;
+	status = "okay";
+};
+
+
diff --git a/arch/arm/dts/rk3568-monopi-r5s.dts b/arch/arm/dts/rk3568-monopi-r5s.dts
new file mode 100644
index 00000000..11cb97b7
--- /dev/null
+++ b/arch/arm/dts/rk3568-monopi-r5s.dts
@@ -0,0 +1,12 @@
+//
+// Copyright (C) 2015 - 2029. Hydra Project.
+//
+
+/dts-v1/;
+#include "rk3568-evb.dts"
+
+/ {
+	model = "Ombres MonoPi R5S";
+	compatible = "Ombres,monopi-r5s", "rockchip,rk3568";
+};
+
